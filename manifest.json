{
	"manifest_version": 2,
	"name": "Fontmagic",
	"description": "Add your own fonts to google chrome bitches",
	"version": "1.0",
	"content_scripts": [{
		"matches": [
		"<all_urls>"
		],
		"js": [ "js/jquery.js", "js/fontmagic.js" ]
	}],
	"options_page": "html/options.html",
	"permissions": [
		"storage",
		"unlimitedStorage"
	]
}
